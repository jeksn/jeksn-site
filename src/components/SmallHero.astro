---
let title = Astro.props.title;
let subtitle = Astro.props.subtitle;
---

<div class="pt-20 px-4 mb-4 bg-zinc-900">
	<div class="max-w-screen-lg text-left space-y-4 mx-auto">
		<h1 class="text-2xl md:text-3xl">{title}</h1>
		<h2 class="text-base md:text-lg text-[#939393]">{subtitle}</h2>
	</div>
</div>